{
  "manifest_version": 3,
  "name": "ChatBooth",
  "description": "ChatBooth app Easy to use",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "./favicon-16.png",
      "48": "./favicon-48.png",
      "128": "./favicon-128.png"
    }
  },
  "background": {
    "service_worker": "chrome/background.js"
  },
  "permissions": [
    "tabs"
  ]
}
